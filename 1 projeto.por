programa {
  funcao inicio() {
    //Declaração de variável
      cadeia nome
      inteiro idade

    //Atribuição direta de variavel
      nome = "diego 808"
      idade = -18

    escreva("nome: ", nome)
    escreva("\nidade: ", idade)

    //Atribuição comando leia()
    escreva("\ndigite seu nome: ")
    leia(nome)
    escreva("digite seu idade: ")
    leia(idade)

  }
}
